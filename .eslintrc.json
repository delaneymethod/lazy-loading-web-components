{
	"root": true,
	"env": {
		"browser": true,
		"es2021": true
	},
	"globals": {
	},
	"ignorePatterns": [
	],
	"extends": "airbnb-base",
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-new": 0,
		"no-tabs": 0,
		"max-len": 0,
		"no-ignore": 0,
		"func-names": 0,
		"no-console": 0,
		"indent": [2, "tab"],
		"no-param-reassign": 0,
		"no-underscore-dangle": 0,
		"class-methods-use-this": 0
	}
}
